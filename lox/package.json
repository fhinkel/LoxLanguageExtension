{
  "name": "lox",
  "displayName": "Lox",
  "description": "VS code extension for Lox language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lox",
      "aliases": ["Lox", "lox"],
      "extensions": [".lox"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lox",
      "scopeName": "source.lox",
      "path": "./syntaxes/lox.tmLanguage.json"
    }]
  }
}
